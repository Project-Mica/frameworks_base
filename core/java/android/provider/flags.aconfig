}

flag {
    name: "enable_documents_trash_api"
    is_exported: true
    namespace: "documentsui"
    is_fixed_read_only: true
    description: "This flag will enable documents trash APIs"
    bug: "409260986"
}

# OWNER=rambowang TARGET=25Q4
flag {
    name: "expose_system_update_settings"
    is_exported: true
