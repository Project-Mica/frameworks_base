<resources>
    <!-- START REQUEST COMPUTER CONTROL ACCESS DIALOG -->

    <style name="RequestComputerControlAccessDialogScrollView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingHorizontal">24dp</item>
        <item name="android:paddingTop">24dp</item>
        <item name="android:paddingBottom">16dp</item>
        <item name="android:background">@color/settingslib_materialColorSurfaceContainerHigh</item>
    </style>

    <style name="RequestComputerControlAccessDialogContent">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center_horizontal</item>
    </style>

    <style name="RequestComputerControlAccessDialogAgentIcon">
        <item name="android:layout_width">60dp</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:paddingTop">12dp</item>
        <item name="android:paddingEnd">4dp</item>
        <item name="android:scaleType">fitCenter</item>
    </style>

    <style name="RequestComputerControlAccessDialogTitle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">24dp</item>
        <item name="android:layout_marginBottom">16dp</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:textAppearance">@style/TextAppearance.SettingsLib.HeadlineSmall</item>
    </style>

    <style name="RequestComputerControlAccessDialogDescription">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">start</item>
        <item name="android:lineSpacingMultiplier">1.2</item>
        <item name="android:textAppearance">@style/TextAppearance.SettingsLib.BodyMedium</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="RequestComputerControlAccessDialogAllowButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">28dp</item>
        <item name="android:minHeight">@dimen/settingslib_expressive_space_medium3</item>
        <item name="android:background">@drawable/settingslib_expressive_button_background_filled</item>
        <item name="android:textAppearance">@style/TextAppearance.SettingsLib.LabelLarge</item>
        <item name="android:textColor">@color/settingslib_materialColorOnPrimary</item>
    </style>

    <style name="RequestComputerControlAccessDialogDontAllowButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/settingslib_expressive_space_small1</item>
        <item name="android:layout_marginBottom">@dimen/settingslib_expressive_space_extrasmall4</item>
        <item name="android:minHeight">@dimen/settingslib_expressive_space_medium3</item>
        <item name="android:background">@drawable/settingslib_expressive_button_background_filled</item>
        <item name="android:textAppearance">@style/TextAppearance.SettingsLib.LabelLarge</item>
        <item name="android:textColor">@color/settingslib_materialColorOnPrimary</item>
    </style>

    <style name="RequestComputerControlAccessDialogAlwaysAllowButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/settingslib_expressive_space_small1</item>
        <item name="android:layout_marginBottom">@dimen/settingslib_expressive_space_extrasmall4</item>
        <item name="android:minHeight">@dimen/settingslib_expressive_space_medium3</item>
        <item name="android:background">@drawable/settingslib_expressive_button_background_outline</item>
        <item name="android:textAppearance">@style/TextAppearance.SettingsLib.LabelLarge</item>
        <item name="android:textColor">@color/settingslib_materialColorPrimary</item>
    </style>

    <!-- END REQUEST COMPUTER CONTROL ACCESS DIALOG -->

    <!-- START NOTIFY COMPUTER CONTROL BLOCKED ACTIVITY DIALOG -->

    <style name="NotifyComputerControlBlockedActivityScrollView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingHorizontal">24dp</item>
        <item name="android:paddingTop">24dp</item>
        <item name="android:paddingBottom">16dp</item>
        <item name="android:background">@color/settingslib_materialColorSurfaceContainerHigh</item>
    </style>

    <style name="NotifyComputerControlBlockedActivityContent">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center_horizontal</item>
    </style>

    <style name="NotifyComputerControlBlockedActivityTitle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">24dp</item>
        <item name="android:layout_marginBottom">16dp</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:textAppearance">@style/TextAppearance.SettingsLib.HeadlineSmall</item>
    </style>

    <style name="NotifyComputerControlBlockedActivityOkButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">28dp</item>
        <item name="android:minHeight">@dimen/settingslib_expressive_space_medium3</item>
        <item name="android:background">@drawable/settingslib_expressive_button_background_filled</item>
        <item name="android:textAppearance">@style/TextAppearance.SettingsLib.LabelLarge</item>
        <item name="android:textColor">@color/settingslib_materialColorOnPrimary</item>
    </style>

    <!-- END NOTIFY COMPUTER CONTROL BLOCKED ACTIVITY DIALOG -->

    <!-- START AUTOMATED APP LAUNCH WARNING ACTIVITY DIALOG -->

    <style name="AutomatedAppLaunchWarningDialogScrollView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingHorizontal">24dp</item>
        <item name="android:paddingTop">24dp</item>
        <item name="android:paddingBottom">16dp</item>
        <item name="android:background">@color/settingslib_materialColorSurfaceContainerHigh</item>
    </style>

    <style name="AutomatedAppLaunchWarningDialogContent">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center_horizontal</item>
    </style>

    <style name="AutomatedAppLaunchWarningDialogIconBar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">12dp</item>
        <item name="android:paddingEnd">4dp</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="AutomatedAppLaunchWarningDialogAgentIcon">
        <item name="android:layout_width">60dp</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:scaleType">fitCenter</item>
    </style>

    <style name="AutomatedAppLaunchWarningDialogTargetIcon">
        <item name="android:layout_width">60dp</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:layout_marginStart">-10dp</item>
        <item name="android:scaleType">fitCenter</item>
    </style>

    <style name="AutomatedAppLaunchWarningDialogTitle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">24dp</item>
        <item name="android:layout_marginBottom">16dp</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:textAppearance">@style/TextAppearance.SettingsLib.HeadlineSmall</item>
    </style>

    <style name="AutomatedAppLaunchWarningDialogDescription">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">start</item>
        <item name="android:lineSpacingMultiplier">1.2</item>
        <item name="android:textAppearance">@style/TextAppearance.SettingsLib.BodyMedium</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="AutomatedAppLaunchWarningDialogButtonBar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">28dp</item>
        <item name="android:gravity">end</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingBottom">@dimen/settingslib_expressive_space_small1</item>
    </style>

    <style name="AutomatedAppLaunchWarningDialogStopButton">
        <item name="android:layout_marginTop">@dimen/settingslib_expressive_space_extrasmall4</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">@dimen/settingslib_expressive_space_medium4</item>
        <item name="android:minWidth">0dp</item>
        <item name="android:paddingVertical">@dimen/settingslib_expressive_space_extrasmall5</item>
        <item name="android:paddingHorizontal">@dimen/settingslib_expressive_space_small1</item>
        <item name="android:stateListAnimator">@null</item>
        <item name="textAllCaps">false</item>
        <item name="android:textAppearance">@style/TextAppearance.SettingsLib.LabelLarge</item>
        <item name="android:textColor">@color/settingslib_materialColorPrimary</item>
        <item name="android:layout_marginEnd">@dimen/settingslib_expressive_space_extrasmall4</item>
        <item name="android:background">@drawable/settingslib_expressive_button_background_outline</item>
        <item name="backgroundTint">@color/settingslib_materialColorOutlineVariant</item>
    </style>

    <style name="AutomatedAppLaunchWarningDialogWaitButton">
        <item name="android:layout_marginTop">@dimen/settingslib_expressive_space_extrasmall4</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">@dimen/settingslib_expressive_space_medium4</item>
        <item name="android:minWidth">0dp</item>
        <item name="android:paddingVertical">@dimen/settingslib_expressive_space_extrasmall5</item>
        <item name="android:paddingHorizontal">@dimen/settingslib_expressive_space_small1</item>
        <item name="android:stateListAnimator">@null</item>
        <item name="textAllCaps">false</item>
        <item name="android:textAppearance">@style/TextAppearance.SettingsLib.LabelLarge</item>
        <item name="android:layout_marginEnd">@dimen/settingslib_expressive_space_extrasmall6</item>
        <item name="android:background">@drawable/settingslib_expressive_button_background_filled</item>
        <item name="backgroundTint">@color/settingslib_materialColorPrimary</item>
        <item name="android:textColor">@color/settingslib_materialColorOnPrimary</item>
    </style>

    <!-- END AUTOMATED APP LAUNCH WARNING ACTIVITY DIALOG -->

</resources>
