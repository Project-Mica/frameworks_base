android_library {
    name: "SettingsLibCollapsingToolbarBaseActivity",
    use_resource_processor: true,
    defaults: [
        "SettingsLibApexDefaults",
        "SettingsLintDefaults",
    ],

    srcs: [
        "src/**/*.java",
        "src/**/*.kt",
