        "SettingsLibSettingsTheme",
        "SettingsLibSettingsTransition",
        "androidx.activity_activity",
        "androidx.annotation_annotation",
        "androidx.core_core",
        "com.google.android.material_material",
    ],
    sdk_version: "system_current",
    min_sdk_version: "29",
}
