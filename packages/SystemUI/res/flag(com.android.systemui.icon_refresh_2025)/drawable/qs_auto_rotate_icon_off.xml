<animated-vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:drawable="@drawable/vd_autorotate">
        <target
            android:name="box_group">
            <aapt:attr name="android:animation">
                <set>
                    <objectAnimator
                        android:propertyName="rotation"
                        android:valueFrom="-45"
                        android:valueTo="-180"
                        android:startOffset="0"
                        android:duration="800"
                        android:interpolator="@anim/overshoot"
                        />
                </set>
            </aapt:attr>
        </target>
    <target android:name="arrow_group_a">
        <aapt:attr name="android:animation">
            <set android:ordering="together">
                <objectAnimator
                    android:duration="800"
                    android:interpolator="@anim/overshoot"
                    android:propertyName="translateXY"
                    android:pathData="M25.1562 21.5C24.6062 21.5 24.4375 21.9492 24.4375 22.4953V41.6659C24.4375 42.212 25.1062 42.6562 25.6562 42.6562H38.7862"
                    android:propertyXName="translateX"
                    android:propertyYName="translateY"
                    android:startOffset="0" />
                <objectAnimator
                    android:duration="15"
                    android:propertyName="rotation"
                    android:valueFrom="45"
                    android:valueTo="-45"
                    android:startOffset="0" />
                <objectAnimator
                    android:duration="15"
                    android:propertyName="rotation"
                    android:valueFrom="-45"
                    android:valueTo="-135"
                    android:startOffset="70" />
            </set>
        </aapt:attr>
    </target>
    <target android:name="arrow_group_b">
        <aapt:attr name="android:animation">
            <set android:ordering="together">
                <objectAnimator
                    android:duration="800"
                    android:interpolator="@anim/overshoot"
                    android:propertyName="translateXY"
                    android:pathData="M38.8 42.6562C39.1148 42.6562 39.5625 42.2085 39.5625 41.6562V22.3438C39.5625 21.7915 39.1148 21.3438 38.5625 21.3438H25.4375"
                    android:propertyXName="translateX"
                    android:propertyYName="translateY"
                    android:startOffset="0" />
                <objectAnimator
                    android:duration="15"
                    android:propertyName="rotation"
                    android:valueFrom="-135"
                    android:valueTo="-225"
                    android:startOffset="0" />
                <objectAnimator
                    android:duration="15"
                    android:propertyName="rotation"
                    android:valueFrom="-225"
                    android:valueTo="-315"
                    android:startOffset="70" />
            </set>
        </aapt:attr>
    </target>
    <target android:name="box_path_a">
        <aapt:attr name="android:animation">
            <set android:ordering="together">
                <objectAnimator
                    android:duration="800"
                    android:interpolator="@anim/overshoot"
                    android:propertyName="trimPathOffset"
                    android:startOffset="0"
                    android:valueFrom="0.412"
                    android:valueTo="0.0" />
                <objectAnimator
                    android:duration="800"
                    android:interpolator="@anim/overshoot"
                    android:propertyName="trimPathStart"
                    android:startOffset="0"
                    android:valueFrom="0.607"
                    android:valueTo="0.55" />
                <objectAnimator
                    android:duration="800"
                    android:interpolator="@anim/overshoot"
                    android:propertyName="trimPathEnd"
                    android:startOffset="0"
                    android:valueFrom="1"
                    android:valueTo="0.91" />
            </set>
        </aapt:attr>
    </target>
    <target android:name="box_path_b">
        <aapt:attr name="android:animation">
            <set android:ordering="together">
                <objectAnimator
                    android:duration="800"
                    android:interpolator="@anim/overshoot"
                    android:propertyName="trimPathOffset"
                    android:startOffset="0"
                    android:valueFrom="0.52"
                    android:valueTo="0.02" />
            </set>
        </aapt:attr>
    </target>
</animated-vector>

